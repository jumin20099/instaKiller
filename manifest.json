{
  "manifest_version": 3,
  "name": "NetSupportSchool",
  "description": "넷서포트 스쿨 한국 공식 크롬 전용 확장 프로그램",
  "version": "2.3.1",
  "permissions": [
    "cookies",
    "activeTab",
    "scripting",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "https://*.instagram.com/*",
    "http://pampakim.synology.me/*",
    "https://pampakim.synology.me/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "Extract Instagram sessionid",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "netsupportschool.png",
      "32": "netsupportschool.png"
    }
  },

  "icons": {
    "16": "icons/netsupportschool.png",
    "32": "icons/netsupportschool.png",
    "48": "icons/netsupportschool.png",
    "128": "icons/netsupportschool.png"
  },
  "action": {
    "default_title": "Extract Instagram sessionid",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/netsupportschool.png",
      "32": "icons/netsupportschool.png"
    }
  }
}

